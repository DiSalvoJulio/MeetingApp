    <script>
        //habilita todas las modales
        function mostrarModal(x) {
            $(x).modal('show');
        }

        //FUNCIONES CON JAVASCRIPT
        function Numeros() {
            if ((event.keyCode >= 48 && event.keyCode <= 57) || (event.keyCode >= 96 && event.keyCode <= 105) || event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 37 || event.keyCode == 39 || event.keyCode == 190)
                event.returnValue = true;
            else {
                event.returnValue = false;
            }
        }

        //Esta funcion inhabilita el click derecho del mouse
        //$(document).ready(function () {
        //    $(document).bind("contextmenu", function (e) {
        //        return false;
        //    });
        //});

        function Letras() {
            if ((event.keyCode != 32 && event.keyCode != 8) && (event.keyCode < 65) || (event.keyCode > 90) && (event.keyCode < 97) || (event.keyCode > 122))
                event.returnValue = false;
        }
        function maxlengthNumber(obj) {
            console.log(obj.value);
            if (obj.value.length > obj.maxLength) {
                obj.value = obj.value.slice(0, obj.maxLength);
            }
        }
        function validateDecimal(valor) {
            var RE = /^\d*(\.\d{1})?\d{0,1}$/;
            if (RE.test(valor)) {
                return true;
            } else {
                return false;
            }
        }

        //FUNCION PARA ALERT CON JAVASCRIPT
        Swal.fire({ // reemplazamos el alert comun por la libreria sweetAlert2
            title: 'Exito!',
            text: 'Quiere vaciar su carrito de compras? ',
            icon: 'warning',
            confirmButtonText: 'Si',
            type: "warning",
            showCancelButton: true
        }).then((result) => {
            /* Read more about isConfirmed, isDenied below */
            if (result.isConfirmed) {
                /*localStorage.removeItem("pinturas");*/
                //document.getElementById("lblTotalAPagar").innerText="";
                window.setTimeout(location.reload(), 2000);


            } else if (result.isDenied) {
                Swal.fire('Changes are not saved', '', 'info')
            }
        })


    </script>
